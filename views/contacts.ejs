<!DOCTYPE html>
<html lang="en">
<%- include('./partials/head.ejs')%>
<body>
  <%- include('./partials/nav.ejs') %>
  <h1><b>Contacts : </b></h1>
  <ul>
    <% if (contacts.length) { %>
      <% contacts.forEach(({ link, name }) => { %>
        <li>
          <a id="a" href="<%= link %>"><%= name %></a>
        </li>
      <% }) %>
    <% } %>
  </ul>
</body>
</html>
